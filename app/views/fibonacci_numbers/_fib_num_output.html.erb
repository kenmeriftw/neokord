<% if FibonacciNumber.last %>
  <%= form_with model: FibonacciNumber.last, remote: true do |form| %>
    <%= form.label "The closest Fibonacci number for #{FibonacciNumber.last&.input_number} is " %>
    <%= form.number_field :output_number, class: 'form-control' %>  
  <% end %>
<% else %>
  <span>You've entered no number yet</span>
<% end %>
